{
  "name": "noflux",
  "version": "2.2.3",
  "description": "a simple top down data flow implementation",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "mocha --require babel-register",
    "coverage": "mocha --require babel-register -R html-cov > coverage.html",
    "coverage-lcov": "mocha --require babel-register -R mocha-lcov-reporter > lcov.info",
    "coverage-json": "mocha --require babel-register -R json-cov > coverage.json",
    "build": "babel lib --out-dir dist && rm -rf dist/__tests__"
  },
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "keywords": [
    "flux",
    "noflux"
  ],
  "author": "liuxijin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "blanket": "^1.1.7",
    "expect.js": "^0.3.1",
    "jsdom": "^5.4.3",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "react": ">0.10",
    "react-addons-test-utils": "^0.14.3",
    "sinon": "^1.14.1"
  },
  "dependencies": {
    "dataton": "^1.6.4",
    "envify": "^3.0.0",
    "semver": "^5.3.0"
  },
  "optionalDependencies": {
    "pure-render-decorator": "^1.2.1"
  },
  "peerDependencies": {
    "react": ">=15.3.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssnau/noflux.git"
  },
  "bugs": {
    "url": "https://github.com/ssnau/noflux/issues"
  },
  "homepage": "https://github.com/ssnau/noflux#readme"
}
